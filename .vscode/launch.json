  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "cwd": "${workspaceFolder}/examples/show-assets",
      "name": "Run Jest - Show Assets",
      "program": "${workspaceRoot}/node_modules/jest-cli/bin/jest.js",
      "args": ["-i"]
    },
    {
      "type": "node",
      "request": "launch",
      "cwd": "${workspaceRoot}/examples/show-errors",
      "name": "Show Errors",
      "program": "${workspaceRoot}/node_modules/cater/bin/cater",
      "args": ["dev"]
    },
    {
      "type": "node",
      "request": "launch",
      "cwd": "${workspaceRoot}/examples/hello-world",
      "name": "Hello World",
      "program": "${workspaceRoot}/node_modules/cater/bin/cater",
      "args": ["dev"]
    }
  ]
}
