{
    "author": "Jon Williams <jon@jonathannen.com> https://jonathannen.com",
    "description": "Workspace for: Modular convention-driven framework for React and Friends.",
    "private": true,
    "version": "0.1.2",
    "workspaces": [
        "examples/*",
        "packages/*"
    ],
    "scripts": {
        "test": "jest --no-cache"
    },
    "jest": {
        "projects": [
            "<rootDir>/examples/*",
            "<rootDir>/packages/*"
        ],
        "transform": {
            "\\.js$": "<rootDir>/packages/cater/test/util.babel-jest.js"
        }
    }
}